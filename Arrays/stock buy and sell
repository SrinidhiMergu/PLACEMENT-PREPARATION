int maxProfit(vector<int>& prices) {
        int mini=INT_MAX, profit=0, n=prices.size();
        for(int i=0; i<n; i++){
            if(prices[i]<mini){
                mini=prices[i];
            }
            
            if(profit<(prices[i] - mini)){
                profit = prices[i] - mini;
            }
        }
        return profit;
    }
    
    
