void sortColors(vector<int>& n) {
        int low=0, mid=0, sz=n.size();
        int high=sz-1;
        while(mid<=high){
            if(n[mid]==0){
                swap(n[low], n[mid]);
                low++, mid++;
            }
            else if(n[mid]==1){
                mid++;
            }
            else if(n[mid]==2){
                swap(n[mid], n[high]);
                high--;
            }
        }
    }
